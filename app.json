{
  "expo": {
    "name": "Fixit",
    "slug": "Fixit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fixit-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/fixit-logo-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asafz.Fixit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fixit-logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/fixit-logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5e9f0f88-d319-4e0f-89d9-e4c0f95b498f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/5e9f0f88-d319-4e0f-89d9-e4c0f95b498f"
    }
  }
}
